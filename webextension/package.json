{
  "name": "scua-extension",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "copydeps": "cp ../thirdparty/ion.min.js build",
    "copyhtml": "cp templates/scua.html build",
    "build": "mkdir -p build && npm run copydeps && npm run copyhtml && rollup -c scua.config.js",
    "dev": "rollup -w -c scua.config.js",
    "start": "sirv . --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@chris.troutner/ipfs-core-types": "^0.6.1",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/svelte": "^2.0.1",
    "@types/webextension-polyfill": "^0.8.0",
    "cids": "^1.1.9",
    "rollup": "^2.56.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.42.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "webextension-polyfill": "^0.8.0"
  }
}
